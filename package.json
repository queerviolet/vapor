{
  "name": "particle-excess-demo",
  "description": "Simulating and rendering 262,144 particles with GLSL",
  "version": "0.0.0",
  "main": "demo.js",
  "browser": "demo.js",
  "scripts": {
    "start": "beefy demo.js:bundle.js --live --debug",
    "prepare": "browserify demo.js > bundle.js"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "stage-2",
            "react"
          ]
        }
      ],
      [
        "glslifyify"
      ]
    ]
  },
  "dependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babelify": "^7.3.0",
    "gl-buffer": "^2.1.2",
    "gl-fbo": "^2.0.5",
    "gl-now": "1.4.0",
    "gl-shader": "4.2.1",
    "gl-texture2d": "^2.1.0",
    "gl-vao": "1.3.0",
    "glsl-noise": "0.0.0",
    "ndarray": "^1.0.18",
    "ndarray-fill": "^1.0.2",
    "zeros": "^1.0.0"
  },
  "devDependencies": {
    "glslifyify": "~0.1.1",
    "beefy": "~2.1.8",
    "browserify": "~14.4.0"
  },
  "author": "Hugh Kennedy <hughskennedy@gmail.com> (http://hughsk.io/)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/hughsk/particle-excess-demo"
  },
  "bugs": {
    "url": "https://github.com/hughsk/particle-excess-demo/issues"
  }
}
